package hilosFMC;

public class MyRunnableClass implements Runnable {

	public MyRunnableClass() {
		// TODO Auto-generated constructor stub
	}

	@Override
	public void run() {
		System.out.println("entrando en el run() Method");
		System.out.println("Haciendo cosas in run() Method");
		System.out.println("Excitando in run89 Method");
	}

	public static void main(String[] args) {
		MyRunnableClass myRunnable =new MyRunnableClass();
		Thread t= new Thread(myRunnable);
		t.start();
//		for(;;)
//		{
//			System.out.println("soy main");
//		}
	}

}
